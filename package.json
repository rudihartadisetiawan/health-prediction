{
  "name": "healthpredict",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:all": "concurrently \"npm run dev:ml\" \"npm run dev:api\" \"npm run dev:web\"",
    "dev:web": "next dev",
    "dev:api": "npm run dev --prefix healthpredict-api",
    "dev:ml": "cd healthpredict-ml && uvicorn main:app --reload --port 8000",
    "start:all": "concurrently \"npm run start:ml\" \"npm run start:api\" \"npm run start:web\"",
    "start:web": "next start",
    "start:api": "npm run start --prefix healthpredict-api",
    "start:ml": "cd healthpredict-ml && uvicorn main:app --port 8000"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "framer-motion": "^12.5.0",
    "lottie-react": "^2.4.1",
    "next": "14.2.35",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "recharts": "^2.15.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2"
  }
}
